#pragma once

#include "src/DataConverter.hpp"

DATACONV_NAMESPACE_BEGIN

/**
 * @brief ライブラリのバージョンを取得する
 * 
 * @return std::string バージョン
 */
inline static std::string getLibVersion() {
    return DATACONV_TO_STRING(DATACONV_VERSION);
}

DATACONV_NAMESPACE_END